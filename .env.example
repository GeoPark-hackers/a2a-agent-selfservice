# Azure Configuration
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=your-resource-group
AZURE_KEYVAULT_NAME=your-keyvault-name
AZURE_CLIENT_ID=your-client-id
AZURE_TENANT_ID=your-tenant-id

# LLM Configuration (Azure OpenAI or Google AI)
LLM_PROVIDER=azure_openai  # Options: azure_openai, google_ai
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-api-key
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o
AZURE_OPENAI_API_VERSION=2024-08-01-preview

# Google AI (alternative)
GOOGLE_API_KEY=your-google-api-key

# Application Configuration
APP_ENV=development
APP_DEBUG=true
APP_HOST=0.0.0.0
APP_PORT=8000
LOG_LEVEL=INFO

# Redis Configuration (for agent state persistence)
REDIS_URL=redis://localhost:6379/0

# A2A Configuration
A2A_SERVICE_URL=http://localhost:8000
A2A_SERVICE_NAME=a2a-agent-selfservice
